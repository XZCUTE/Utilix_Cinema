{
  "buildCommand": "cd client && npm install && vite build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "routes": [
    {
      "pattern": "/*",
      "script": null,
      "serve": "/index.html",
      "status": 200
    }
  ],
  "headers": {
    "/*": {
      "X-Frame-Options": "DENY",
      "X-Content-Type-Options": "nosniff",
      "Referrer-Policy": "no-referrer"
    }
  }
}
